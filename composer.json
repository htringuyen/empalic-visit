{
  "name": "slimmvc/website",
  "description": "a custom mvc framework to serve crud app",
  "scripts": {
    "serve": "/opt/lampp/bin/php -S 127.0.0.1:8000 -t public"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Framework\\": "framework"
    },
    "files": [
      "framework/helpers.php"
    ]
  },
  "config": {
    "process-timeout": 0
  },
  "requires": {
    "filb/whoops": "^2.15.4",
    "vlucas/phpdotenv": "v5.5.0"
  }
}